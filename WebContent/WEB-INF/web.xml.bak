<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>info</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>aaa</filter-name>
    <filter-class>com.info.filter.EncodingFilter</filter-class>
    <init-param>
      <param-name>encode</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>aaa</filter-name>
    <url-pattern>/</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>SignIn</servlet-name>
    <servlet-class>com.testServlet.servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignIn</servlet-name>
    <url-pattern>/info/SignIn</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SignUp</servlet-name>
    <servlet-class>com.info.servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignUp</servlet-name>
    <url-pattern>/info/SignUp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PreLoad</servlet-name>
    <servlet-class>com.info.servlet.PreLoad</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PreLoad</servlet-name>
    <url-pattern>/load</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InfoPublish</servlet-name>
    <servlet-class>com.info.servlet.InfoPublish</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InfoPublish</servlet-name>
    <url-pattern>/info/InfoPublish</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InfoManage</servlet-name>
    <servlet-class>com.info.servlet.InfoManage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InfoManage</servlet-name>
    <url-pattern>/info/InfoManage</url-pattern>
  </servlet-mapping>
</web-app>